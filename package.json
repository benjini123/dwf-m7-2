{
  "name": "algolia",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:fe": "parcel serve ./fe-src/index.html",
    "build:fe": "parcel build ./fe-src/index.html --dist-dir ./dist",
    "dev:be": "nodemon --watch 'be-src/**/*' -e ts --exec 'ts-node ./be-src/index.ts' ",
    "build:be": "tsc",
    "start": "node ./be-dist/be-src/index.ts",
    "build": "yarn run build:be && yarn run build:fe",
    "sync": "ts-node ./be-src/sync.ts"
  },
  "dependencies": {
    "@parcel/transformer-image": "^2.6.2",
    "@sendgrid/mail": "^7.7.0",
    "@types/node": "^18.0.3",
    "@vaadin/router": "^1.7.4",
    "algolia": "^0.0.0",
    "algoliasearch": "^4.13.0",
    "cloudinary": "^1.30.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mapbox": "^1.0.0-beta10",
    "mapbox-gl": "^2.9.1",
    "nodemon": "^2.0.15",
    "parcel": "^2.6.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0",
    "tsc": "^2.0.4",
    "typescript": "4.6.4"
  }
}
